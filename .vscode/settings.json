{
  "editor.formatOnSave": true,
  "python.formatting.provider": "yapf",
  "python.linting.enabled": true,
  "[python]": {
    "editor.wordBasedSuggestions": false,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
  },
  "[json]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.detectIndentation": false,
    "editor.suggest.insertMode": "replace",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
  },
  "[jsonc]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.detectIndentation": false,
    "editor.suggest.insertMode": "replace",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
  },
  "files.associations": {
    "*.ejs": "html",
    "*.js": "javascriptreact",
    "*.tcc": "cpp",
    "cctype": "cpp",
    "clocale": "cpp",
    "cmath": "cpp",
    "compare": "cpp",
    "concepts": "cpp",
    "cstdint": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "cstring": "cpp",
    "cwchar": "cpp",
    "exception": "cpp",
    "initializer_list": "cpp",
    "iosfwd": "cpp",
    "limits": "cpp",
    "new": "cpp",
    "stdexcept": "cpp",
    "streambuf": "cpp",
    "string": "cpp",
    "string_view": "cpp",
    "system_error": "cpp",
    "type_traits": "cpp",
    "typeinfo": "cpp",
    "utility": "cpp"
  },
  "python.analysis.extraPaths": [
    "./mujin_server"
  ],
}